{
  "name": "root",
  "private": true,
  "description": "Statik plugin for manage Gutenberg Blocks.",
  "license": "GPL-3.0-or-later",
  "config": {
    "IS_GUTENBERG_PLUGIN": true
  },
  "engines": {
    "node": "16"
  },
  "workspaces": [
    "packages/@statik-space/wordpress-statik-blocks"
  ],
  "scripts": {
    "build": "npm run packages:build && npm run editor:build && npm run blocks:build",
    "blocks:start": "wp-scripts start --config-name blocks",
    "blocks:prebuild": "node ./tools/block-icon-prebuild.js",
    "blocks:build": "wp-scripts build --config-name blocks",
    "editor:start": "wp-scripts start --config-name editor",
    "editor:build": "wp-scripts build ./editor/index.js --config-name editor",
    "packages:build": "npm run build --workspaces --if-present",
    "check-engines": "wp-scripts check-engines",
    "lint:css": "wp-scripts lint-style",
    "lint:css:fix": "wp-scripts lint-style --fix",
    "lint:js": "wp-scripts lint-js",
    "lint:js:fix": "wp-scripts lint-js --fix",
    "lint:php": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix --dry-run",
    "lint:php:fix": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix",
    "format": "wp-scripts format",
    "packages-update": "wp-scripts packages-update",
    "test:e2e:debug": "wp-scripts --inspect-brk test-e2e --config packages/e2e-tests/jest.config.js --puppeteer-devtools",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:e2e": "wp-scripts test-e2e",
    "wp-env": "wp-env",
    "wp-env:prestart": "bash ./tools/wp-env-prestart.sh",
    "wp-env:start": "wp-env start",
    "wp-env:poststart": "bash ./tools/wp-env-poststart.sh",
    "wp-env:stop": "wp-env stop",
    "wp-env:clean": "wp-env clean all",
    "wp-env:destroy": "wp-env destroy",
    "cleanup": "rm -rf ./blocks/*/*/build && rm -rf ./editor/build && rm -rf ./packages/@statik-space/*/build"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@wordpress/base-styles": "^4.18.0",
    "@wordpress/block-editor": "^11.4.0",
    "@wordpress/blocks": "^12.4.0",
    "@wordpress/components": "^23.4.0",
    "@wordpress/data": "^8.4.0",
    "@wordpress/e2e-test-utils": "^9.4.0",
    "@wordpress/element": "^5.4.0",
    "@wordpress/env": "^5.12.0",
    "@wordpress/eslint-plugin": "^14.0.0",
    "@wordpress/hooks": "^3.27.0",
    "@wordpress/i18n": "^4.27.0",
    "@wordpress/icons": "^9.18.0",
    "@wordpress/keycodes": "^3.27.0",
    "@wordpress/media-utils": "^4.18.0",
    "@wordpress/plugins": "^5.4.0",
    "@wordpress/postcss-plugins-preset": "^4.11.0",
    "@wordpress/scripts": "^25.4.0",
    "@wordpress/stylelint-config": "^21.10.0",
    "camelcase": "^6.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "expose-loader": "^4.0.0",
    "fs-extra": "^11.1.0",
    "globby": "^11.0.4",
    "uuid": "^3.3.3",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/modifiers": "^6.0.0",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@react-google-maps/api": "^2.13.1",
    "@statik-space/wordpress-statik-blocks": "4.13.0-beta.0",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "fast-average-color": "9.1.1",
    "iconoir": "^6.2.1",
    "lodash": "^4.17.21",
    "memize": "^1.1.0",
    "micromodal": "^0.4.10",
    "query-string": "^7.1.1",
    "react-window": "^1.8.8",
    "remove-accents": "^0.4.3",
    "swiper": "^8.4.2",
    "webpack-merge": "^5.8.0"
  }
}
